{% trans_default_domain 'FOSUserBundle' %}
{% form_theme form ":FormTemplate:bootstrap.html.twig" %}
<div class="row">
    <section class="col-sm-offset-2 col-sm-8 col-xs-12 backWhite sec1">
        <div class="row">
            <div class="col-sm-offset-3 col-sm-6">
                <h2>Inscription</h2>
                {{ form_start(form, {'method': 'post', 'action': path('booster_registerActor'), 'attr': {'class': 'fos_user_registration_register'}}) }}
                {{ form_widget(form) }}
                <div>
                    <input type="submit" value="{{ 'registration.submit'|trans }}" class="btn btn-success center-block successButton" />
                </div>

                {{ form_end(form) }}
            </div>
        </div>
    </section>
</div>